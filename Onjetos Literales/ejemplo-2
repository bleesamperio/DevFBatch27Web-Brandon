const MicuentaDeAlumno = {

    // idealmente, si le pones "" o '' a un atributo, para mantener tu base de codigo/codebase uniforme, tienes que a√±adiselas a los demas atributos.

    "id": "Al0171389DevF",
    "nombre completo": "Brandon", //Contenido con espacios
    "Materias": ["HTML","CSS","JS"],
    "MostrarGrupo"(){
        return `El alumno ${this["nombre completo"]}, se encuentra cursando la materia ${this.Materia[3]}`;
    }
};

